{
    "configurations": [
        {
            "name": "Linux Kernel",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/include/asm-generic",
                "${workspaceFolder}/include",
                "${workspaceFolder}/arch/x86/include",
                "${workspaceFolder}/scripts",
                "${workspaceFolder}/tools/include"
            ],
            "defines": [
                "__KERNEL__",
                "CONFIG_X86_64"
            ],
            "compilerPath": "/usr/bin/gcc",
            "intelliSenseMode": "gcc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}
